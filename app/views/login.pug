extends template

block content
	#js-error-message.error-message
	form(name='login')
		.form-box
			h2 Please Log in to Continue
			.fb-item
				span Email
				br
				input(type='email' name='email' placeholder='example@gmail.com')
			.fb-item
				span Password
				br 
				input(type='password' name='password' placeholder='******')
			button#submit(type='button' onClick='loginUser()') Login
